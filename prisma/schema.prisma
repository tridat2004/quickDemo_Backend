datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Article {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  title          String
  
  thumbnail      String
  category       String
  published_time String 
   @@map("crawl_data_he")
}
